-- @nsURI UML=http://www.eclipse.org/uml2/5.0.0/UML
-- @nsURI UML2=http://www.eclipse.org/uml2/5.0.0/UML

module Families2Persons;
create OUT:UML2 from IN:UML;

rule State2Interaction {
	from
		s:UML!State
	to
		t:UML2!Interaction (
			name <- s.name
		)
}

rule Transition2Message {
	from
		s:UML!Transition
	to
		t:UML2!Message (
			name <- s.name
		)
}